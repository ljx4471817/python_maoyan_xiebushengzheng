doctype html
html
  head
    title #{Object.keys(FILE_MAP).length} regions
    meta(charset="utf-8")
    style(type="text/css").
      .map{
         width: 300px;
         height: 300px;
      }
      body {
         background-color: #eee;
      }
    script(src="echarts.min.js")
    each value, index in FILE_MAP
      script(src=JS_FOLDER+"/"+value+".js")
  body
    table
      tr
        - var n =0
        each file, index in FILE_MAP
          if n % 4 == 0
            | </tr><tr>
          td
            div(id=file,class="map")
          - n++
    script(src="demo.js")
    script
      each region, index in PINYIN_MAP
        | make_map("#{index}","#{region}");
        |
